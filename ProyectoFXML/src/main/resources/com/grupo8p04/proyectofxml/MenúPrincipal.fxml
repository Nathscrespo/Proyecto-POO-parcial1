<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="200.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.grupo8p04.proyectofxml.MenúPrincipalController">
   <center>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="adminConc" mnemonicParsing="false" onAction="#cambiarAdminConcurso" text="Administrar Concursos">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin></Button>
            <Button fx:id="adminDueños" mnemonicParsing="false" onAction="#cambiarAdminDueños" text="Administrar Dueños">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin></Button>
            <Button fx:id="adminMascotas" mnemonicParsing="false" onAction="#cambiarAdminMascotas" text="Administrar Mascotas" />
         </children>
      </VBox>
   </center>
   <top>
      <Label text="Menú Principal" translateY="10.0" BorderPane.alignment="BOTTOM_CENTER">
         <font>
            <Font name="System Bold" size="25.0" />
         </font></Label>
   </top>
</BorderPane>
